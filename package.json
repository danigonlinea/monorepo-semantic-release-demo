{
  "name": "monorepo-semantic-release-demo",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "release:all": "yarn release:components && yarn release:tokens",
    "release:components": "yarn workspace components-package semantic-release --ci false --debug --dry-run -e semantic-release-monorepo",
    "release:tokens": "yarn workspace tokens-package semantic-release --ci false --debug --dry-run -e semantic-release-monorepo"
  },
  "keywords": [
    "monorepo",
    "semantic-release"
  ],
  "author": "Daniel González Jiménez",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "coa": "2.0.2"
  },
  "devDependencies": {
    "prettier": "^2.8.1",
    "dotenv": "^16.0.3",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "concurrently": "^7.6.0",
    "semantic-release": "^19.0.5",
    "semantic-release-monorepo": "^7.0.5"
  }
}
